{
    "manifest_version": 3,
    "name": "Close Unpinned Tabs",
    "version": "1.0",
    "description": "Closes all unpinned tabs in the current window",
    "permissions": ["tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }